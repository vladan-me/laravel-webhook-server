{
    "name": "spatie/laravel-webhook-server",
    "description": "Send webhooks in Laravel apps",
    "keywords": [
        "spatie",
        "laravel-webhook-server",
        "webhook",
        "server"
    ],
    "homepage": "https://github.com/spatie/laravel-webhook-server",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/bus": "^5.8|^6.0|^7.0",
        "illuminate/queue": "^5.8|^6.0|^7.0",
        "illuminate/support": "^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.4|^9.0",
        "spatie/test-time": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\WebhookServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\WebhookServer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\WebhookServer\\WebhookServerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
